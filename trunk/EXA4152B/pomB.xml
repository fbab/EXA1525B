<build>
<plugins>
  <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>exec-maven-plugin</artifactId>
        <version>1.1</version>
        <executions>
          <execution>
            <goals>
              <goal>exec</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
			<mainClass>org.prodageo.Main</mainClass>
        </configuration>
  </plugin>
</plugins>
</build>


  